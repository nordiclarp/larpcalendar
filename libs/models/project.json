{
  "root": "libs/models",
  "sourceRoot": "libs/models/src",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/models/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/models"],
      "options": {
        "jestConfig": "libs/models/jest.config.js",
        "passWithNoTests": true
      }
    },

    "prep": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "commands": ["prisma migrate dev", "prisma db seed"],
        "cwd": "libs/models",
        "envFile": ".env"
      },
      "configurations": {
        "test": {
          "envFile": ".env.test"
        }
      }
    },

    "migrate-dev": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma migrate dev",
        "cwd": "libs/models",
        "envFile": ".env"
      },
      "configurations": {
        "test": {
          "envFile": ".env.test"
        }
      }
    },
    "migrate-reset": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma migrate reset",
        "cwd": "libs/models",
        "envFile": ".env"
      },
      "configurations": {
        "test": {
          "envFile": ".env.test"
        }
      }
    },
    "migrate-up": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma migrate up",
        "cwd": "libs/models",
        "envFile": ".env"
      },
      "configurations": {
        "test": {
          "envFile": ".env.test"
        }
      }
    },
    "db-seed": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma db seed",
        "cwd": "libs/models",
        "envFile": ".env"
      },
      "configurations": {
        "test": {
          "envFile": ".env.test"
        }
      }
    },
    "generate": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma generate",
        "cwd": "libs/models"
      }
    }
  },
  "tags": []
}
